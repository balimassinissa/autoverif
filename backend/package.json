{
    "name": "autoverif-backend",
    "version": "0.1.0",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev --name init",
        "prisma:studio": "prisma studio",
        "seed": "dotenv -e .env -- ts-node prisma/seed.ts"
    },
    "dependencies": {
        "@prisma/client": "^5.0.0",
        "aws-sdk": "^2.1360.0",
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.0.0",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.0",
        "multer": "^1.4.5-lts.1",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^3.0.0",
        "@types/express": "^5.0.3",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.5.2",
        "dotenv-cli": "^10.0.0",
        "prisma": "^5.0.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6"
    }
}
